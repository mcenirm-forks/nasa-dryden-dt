<monitor destURLPath="http://localhost/RBNB/B200" readTimeout="15000"
		mkcolQuery="?archive=8640000&amp;cache=10&amp;mode=create" debug="true">
	<auth><mode>BASIC</mode><user>rbnb</user><password>rbnb</password></auth>
	
	<!-- Get the full vectors once every 120 sec -->
	<resource>
	    <minimumInterval>120000</minimumInterval>
	    <maximumInterval>120000</maximumInterval>
	    <url>http://indscore.dfrc.nasa.gov/RBNB/B200/B200_ASR.txt</url>
	</resource>
	<resource>
            <minimumInterval>120000</minimumInterval>
            <maximumInterval>120000</maximumInterval>
            <url>http://indscore.dfrc.nasa.gov/RBNB/B200/B200_DEP.txt</url>
        </resource>
	
	<!-- Get the reduced ASR vector once every 20 sec -->
	<resource>
	    <minimumInterval>20000</minimumInterval>
	    <maximumInterval>20000</maximumInterval>
	    <url>http://indscore.dfrc.nasa.gov/RBNB/INDS5/HASR/_CSV</url>
	    <destFile>B200_ASR_reduced.txt</destFile>
	</resource>
	
</monitor>

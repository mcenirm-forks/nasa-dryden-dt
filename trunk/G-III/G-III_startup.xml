<?xml version="1.0" encoding="UTF-8"?>

<!-- G-III project -->

<!--startup xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://whf/schema/inds_startup.xsd"-->
<startup xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file:///C:/user/DryScan/nasa-dryden-dt/G-III/inds_startup.xsd">
	
	<!-- Tomcat -->
	<!-- Doesn't work with the new Tomcat (apache-tomcat-6.0.18) -->
	<!-- tomcat initialDirectory="Tomcat" logFile="tommy.log">
		<input type="tcp" port="localhost:80" />
	</tomcat>
	<sleep duration_ms="5000"/-->
	
	<!-- Server -->
	<del initialDirectory="Server" file="rbnb.log"/>
	<dataTurbine jvmMaxHeap="1024M" initialDirectory="Server" logFile="rbnb.log" name="INDS" address=":3333" tag="G-III_DT"/>
	<sleep duration_ms="15000"/>
	
	<!-- Jobs/PlugIns -->
	<del initialDirectory="Jobs/PlugIns" file="*.log"/>
	<png initialDirectory="Jobs/PlugIns" logFile="png.log" width="320" height="200">
	    <plugin type="rbnb" port="localhost:3333" name="PNGPlugIn" />
	</png>
	<toString initialDirectory="Jobs/PlugIns" logFile="toString.log">
	    <plugin type="rbnb" port="localhost:3333" name="ToString" />
	</toString>
	<thumbNail initialDirectory="Jobs/PlugIns" logFile="thumbNail.log" scale="0.5" quality="0.9" maxImages="10">
	    <plugin type="rbnb" port="localhost:3333" name="ThumbNail" />
	</thumbNail>
	<sleep duration_ms="3000"/>
	
	<!-- Start rbnbFolder -->
	<del initialDirectory="Capture/rbnbFolder" file="*.log"/>
	<runProcess tag="jplimages" classification="source" initialDirectory="../RBNB/V3.2B3/bin" executable="/usr/bin/java" arguments="-jar folder.jar -a localhost -f /usr/local/JPLimages -m create -D" />
	<sleep duration_ms="15000"/>
	
	<!--                                -->
	<!-- UDPCapture/UDPCaster processes -->
	<!--                                -->
	
	<!-- DCAPS (IWG1 packet) -->
	<del initialDirectory="CaptureCaster/DCAPS" file="*.log"/>
	<udpCapture initialDirectory="CaptureCaster/DCAPS" logFile="DCAPS-capture.log">
	    <input type="udp" port="5800"/>
	    <output type="rbnb" port="localhost:3333" cacheFrames="10" archiveFrames="8640000" overwriteArchive="true" name="DCAPS-cap"/>
	</udpCapture>
	<runProcess tag="DCAPS-caster" classification="plugin" logFile="DCAPS-caster.log" initialDirectory="../RBNB/V3.2B3/bin" executable="/usr/bin/java" arguments="-jar udpcaster.jar -c DCAPS-cap/UDP -g -i -r 130.134.183.40:5900 -x" />
	
	<!-- UAVSAR out sending to port 5902 -->
	<del initialDirectory="CaptureCaster/UAVSAR-5902" file="*.log"/>
	<udpCapture initialDirectory="CaptureCaster/UAVSAR-5902" logFile="UAVSAR-5902-capture.log">
	    <input type="udp" port="5902"/>
	    <output type="rbnb" port="localhost:3333" cacheFrames="10" archiveFrames="8640000" overwriteArchive="true" name="UAVSAR-5902-cap"/>
	</udpCapture>
	<runProcess tag="UAVSAR-5903-caster" classification="plugin" logFile="UAVSAR-5903-caster.log" initialDirectory="../RBNB/V3.2B3/bin" executable="/usr/bin/java" arguments="-jar udpcaster.jar -c UAVSAR-5902-cap/UDP -g -i -r 130.134.183.40:5903 -x" />
	
	<!-- UAVSAR out sending to port 5904 -->
	<del initialDirectory="CaptureCaster/UAVSAR-5904" file="*.log"/>
	<udpCapture initialDirectory="CaptureCaster/UAVSAR-5904" logFile="UAVSAR-5904-capture.log">
	    <input type="udp" port="5904"/>
	    <output type="rbnb" port="localhost:3333" cacheFrames="10" archiveFrames="8640000" overwriteArchive="true" name="UAVSAR-5904-cap"/>
	</udpCapture>
	<runProcess tag="UAVSAR-5905-caster" classification="plugin" logFile="UAVSAR-5905-caster.log" initialDirectory="../RBNB/V3.2B3/bin" executable="/usr/bin/java" arguments="-jar udpcaster.jar -c UAVSAR-5904-cap/UDP -g -i -r 130.134.183.40:5905 -x" />
	
	<!-- UAVSAR comm link -->
	<del initialDirectory="CaptureCaster/UAVSAR-5907" file="*.log"/>
	<udpCapture initialDirectory="CaptureCaster/UAVSAR-5907" logFile="UAVSAR-5907-capture.log">
	    <input type="udp" port="5907"/>
	    <output type="rbnb" port="localhost:3333" cacheFrames="10" archiveFrames="8640000" overwriteArchive="true" name="UAVSAR-5907-cap"/>
	</udpCapture>
	<runProcess tag="UAVSAR-5906-caster" classification="plugin" logFile="UAVSAR-5906-caster.log" initialDirectory="../RBNB/V3.2B3/bin" executable="/usr/bin/java" arguments="-jar udpcaster.jar -c UAVSAR-5907-cap/UDP -g -i -r 192.168.8.100:5906 -x" />
	
	<!-- UAVSAR input command line -->
	<del initialDirectory="CaptureCaster/UAVSAR-5909" file="*.log"/>
	<udpCapture initialDirectory="CaptureCaster/UAVSAR-5909" logFile="UAVSAR-5909-capture.log">
	    <input type="udp" port="5909"/>
	    <output type="rbnb" port="localhost:3333" cacheFrames="10" archiveFrames="8640000" overwriteArchive="true" name="UAVSAR-5909-cap"/>
	</udpCapture>
	<runProcess tag="UAVSAR-5908-caster" classification="plugin" logFile="UAVSAR-5908-caster.log" initialDirectory="../RBNB/V3.2B3/bin" executable="/usr/bin/java" arguments="-jar udpcaster.jar -c UAVSAR-5909-cap/UDP -g -i -r 192.168.8.100:5908 -x" />
	
	<sleep duration_ms="15000"/>
	
	<!-- Start MakeTimeMirror -->
	<del initialDirectory="Jobs/MakeTimeMirror" file="*.log"/>
	<runProcess tag="TimeMirror" classification="source" initialDirectory="../INDS-Utility/MakeTimeMirror" executable="/usr/bin/java" arguments="-cp /usr/local/software/INDSExecutionManager/trunk/INDS-Utility/MakeTimeMirror:/usr/local/software/INDSExecutionManager/trunk/RBNB/V3.2B3/bin/rbnb.jar MakeTimeMirror localhost:3333 Folder 130.134.86.73:3333 jplimages" />
	
</startup>

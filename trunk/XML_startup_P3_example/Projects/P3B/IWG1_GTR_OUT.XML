<?xml version="1.0"?>
<!-- IWG1 telemetry stream for GTR -->
<!-- Note this has IWG1 parameters in order, but has different cookie and is binary -->
<output_format xmlns="urn:xmlns:reveal">
  <target>udp://130.134.183.40:5700</target>
  <parameter id="Marker"><type>marker</type><format>GTR_P3orion_IWG1</format></parameter>
  <parameter id="TimeStamp"><type>double</type></parameter>
  <parameter id="Latitude"><type>double</type></parameter>
  <parameter id="Longitude"><type>double</type></parameter>
  <parameter id="GPS_Alt_MSL"><type>float</type></parameter>
  <parameter id="GPS_Altitude"><type>float</type></parameter>
  <parameter id="Pressure_Altitude"><type>float</type></parameter>
  <parameter id="RADAR_Altitude"><type>float</type></parameter>
  <parameter id="Ground_Speed"><type>float</type></parameter>
  <parameter id="True_Air_Speed"><type>float</type></parameter>
  <parameter id="Indicated_Air_Speed"><type>float</type></parameter>
  <parameter id="Mach_Number"><type>float</type></parameter>
  <parameter id="Vertical_Speed"><type>float</type></parameter>
  <parameter id="True_Heading"><type>float</type></parameter>
  <parameter id="Track_Angle"><type>float</type></parameter>
  <parameter id="Drift_Angle"><type>float</type></parameter>
  <parameter id="Pitch_Angle"><type>float</type></parameter>
  <parameter id="Roll_Angle"><type>float</type></parameter>
  <parameter id="Slip_Angle"><type>float</type></parameter>
  <parameter id="Attack_Angle"><type>float</type></parameter>
  <parameter id="Static_Air_Temp"><type>float</type></parameter>
  <parameter id="Dew_Point"><type>float</type></parameter>
  <parameter id="Total_Air_Temp"><type>float</type></parameter>
  <parameter id="Static_Pressure"><type>float</type></parameter>
  <parameter id="Dynamic_Pressure"><type>float</type></parameter>
  <parameter id="Cabin_Pressure"><type>float</type></parameter>
  <parameter id="Wind_Speed"><type>float</type></parameter>
  <parameter id="Wind_Direction"><type>float</type></parameter>
  <parameter id="Vert_Wind_Speed"><type>float</type></parameter>
  <parameter id="Solar_Zenith_Angle"><type>float</type></parameter>
  <parameter id="Aircraft_Sun_Elevation"><type>float</type></parameter>
  <parameter id="Sun_Azimuth"><type>float</type></parameter>
  <parameter id="Aircraft_Sun_Azimuth"><type>float</type></parameter>
  <parameter id="GPS_Vertical_Speed"><type>float</type></parameter>
  <parameter id="ADC_Baro_Altitude"><type>float</type></parameter>
  <parameter id="Potential_Temp"><type>float</type></parameter>
  <parameter id="Cabin_Temp"><type>float</type></parameter>
  <parameter id="Cabin_Humidity"><type>float</type></parameter>
  <parameter id="Acceleration_X"><type>float</type></parameter>
  <parameter id="Acceleration_Y"><type>float</type></parameter>
  <parameter id="Acceleration_Z"><type>float</type></parameter>
</output_format>
